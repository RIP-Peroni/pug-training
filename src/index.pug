-
    const users = {
      admin: [
          {
              name: 'Кодовёнок',
              surname: 'Хекслетович',
          },
      ],
      moderator: [
          {
              name: 'Король',
              surname: 'Вёрстки',
          },
          {
              name: 'Алексей',
              surname: 'Примадонин',
          },
      ],
    };

doctype html
html(lang='ru')
    head
        meta(charset='UTF-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport' content='width=device-width, initial-scale=1.0')
        title Hexlet Profile
    body
        section.container
            each people, position in users
                h2= position
                ul
                    each user in people
                        li= user.name + ' ' + user.surname
